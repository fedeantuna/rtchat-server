{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "https://localhost:3000",
      "http://localhost:3000"
    ]
  },
  "InMemoryCache": {
    "SizeLimit": 14336
  },
  "Auth0": {
    "Domain": "YOUR_AUTH0_DOMAIN",
    "Audience": "YOUR_AUTH0_APP_AUDIENCE"
  },
  "Auth0ManagementAPI": {
    "BaseAddress": "YOUR_AUTH0_MANAGEMENT_API_BASE_ADDRESS",
    "TokenEndpoint": "YOUR_AUTH0_MANAGEMENT_API_TOKEN_ENDPOINT",
    "Audience": "YOUR_AUTH0_MANAGEMENT_API_AUDIENCE",
    "ClientId": "YOUR_AUTH0_MANAGEMENT_API_CLIENT_ID",
    "ClientSecret": "YOUR_AUTH0_MANAGEMENT_API_CLIENT_SECRET",
    "UsersByEmailEndpoint": "YOUR_AUTH0_MANAGEMENT_API_USERS_BY_EMAIL_ENDPOINT",
    "UsersByIdEndpoint": "YOUR_AUTH0_MANAGEMENT_API_USERS_BY_ID_ENDPOINT"
  }
}
